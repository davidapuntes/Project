<Window x:Class="StylesDictionary.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StylesDictionary"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Style="{StaticResource TitleLabel}">Title label</Label>
        <Button Content="Submit Button" Style="{StaticResource CancelButton}"/>
        <Button Content="Cancel Button" Style="{StaticResource SubmitButton}"/>
            <Label Style="{StaticResource StandardLabel}">Secondary label</Label>
        </StackPanel>

    </Grid>
</Window>
